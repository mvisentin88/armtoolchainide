{
	"name": "arm-toolchain",
	"displayName": "arm toolchain",
	"description": "this is a simple toolchain for compile with personal gcc",
	"version": "0.0.1",
	"publisher": "mvisentin",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"keybindings": [
			{
				"command": "arm.build",
				"key": "ctrl+B"
			},
			{
				"command": "arm.build_and_clean",
				"key": "ctrl+shift+B"
			}
		],
		"commands":
		[
			{
			"command": "arm.project-settings",
			"title": "Project Settings"
			},
			{
				"command": "arm.exclude-from-build",
				"title": "Exclude From Build"
			}
		],
		"menus": 
		{
			"explorer/context": 
			[
				{
					"group": "navigation",
					"command": "arm.project-settings"
				},
				{
					"group": "z_commands",
					"command": "arm.exclude-from-build"
				}
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mvisentin88/armtoolchainide.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"fs-extra": "^8.0.1"
	}
}
